	<queryString>
		<![CDATA[SELECT SUM(pc.`product_quantity` ) AS total_sales_category, cp.`description_category` AS description, SUM(pc.`product_quantity` * pc.`product_price` ) AS value_total_sales_category FROM tbl_product p INNER JOIN tbl_product_coupon pc ON p.`prod_bar_code_product` = pc.`prod_bar_code_product`
 INNER JOIN tbl_coupon c ON pc.`idtbl_coupon` = c.`idtbl_coupon` INNER JOIN tbl_category_product cp ON p.`idtbl_category_product` = cp.`idtbl_category_product`WHERE DATE_FORMAT(c.`date_hour_coupon`, '%Y') = $P{PAR_YEAR} GROUP BY p.`idtbl_category_product` ORDER BY cp.`description_category`;]]>
	</queryString>
